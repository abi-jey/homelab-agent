# Environment variables for local development
# Copy this to .env and fill in your API keys

# Runtime directory for local development (user mode)
HOMELAB_AGENT_RUNTIME_DIR=/var/lib/homelab-agent

# Google AI API key (get from https://aistudio.google.com)
GOOGLE_API_KEY=

# OpenAI API key (optional, if using OpenAI)
OPENAI_API_KEY=

# Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=
